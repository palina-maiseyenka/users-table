<mvc:View
	controllerName="palina.maiseyenka.controller.UsersList"
	xmlns="sap.uxap"
	xmlns:m="sap.m"
	xmlns:layout="sap.ui.layout"
    xmlns:core="sap.ui.core"
	xmlns:mvc="sap.ui.core.mvc"
	displayBlock="true">

	<ObjectPageLayout>
		<headerTitle>
			<ObjectPageDynamicHeaderTitle>
				<expandedHeading>
					<m:Title text="{i18n>homePageTitle}" wrapping="true"/>
				</expandedHeading>
				<snappedHeading>
					<m:Title text="{i18n>homePageTitle}" wrapping="true"/>
				</snappedHeading>
			</ObjectPageDynamicHeaderTitle>
		</headerTitle>
		<headerContent>
			<core:Fragment fragmentName="palina.maiseyenka.view.fragments.FilterBar" type="XML"/>
		</headerContent>
		<sections>
			<ObjectPageSection showTitle="false">
				<subSections>
					<ObjectPageSubSection>
						<blocks>							
							<core:Fragment fragmentName="palina.maiseyenka.view.fragments.UsersTable" type="XML"/>
						</blocks>
					</ObjectPageSubSection>					
				</subSections>
			</ObjectPageSection>
		</sections>
	</ObjectPageLayout>
</mvc:View>