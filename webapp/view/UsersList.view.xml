<mvc:View
	controllerName="palina.maiseyenka.controller.UsersList"
	xmlns="sap.m"
	xmlns:mvc="sap.ui.core.mvc"	
    xmlns:core="sap.ui.core"
	xmlns:f="sap.f"
	xmlns:vm="sap.ui.comp.variants"
	displayBlock="true">
	<f:DynamicPage>
		<f:title>
			<f:DynamicPageTitle>
				<f:heading>
					<Title text="{i18n>homePageTitle}"/>		
				</f:heading>
			</f:DynamicPageTitle>
		</f:title>
		<f:header>
			<f:DynamicPageHeader pinnable="true">
				<f:content>
					<core:Fragment fragmentName="palina.maiseyenka.view.fragments.FilterBar" type="XML"/>
				</f:content>
			</f:DynamicPageHeader>
		</f:header>
		<f:content>
			<core:Fragment fragmentName="palina.maiseyenka.view.fragments.UsersTable" type="XML"/>
		</f:content>
	</f:DynamicPage>
</mvc:View>